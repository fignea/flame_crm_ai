{
  "name": "flame-ai-backend",
  "version": "1.0.0",
  "description": "Backend API para Flame AI CRM - Sistema de gestiÃ³n de WhatsApp con CRM",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "db:init": "node scripts/init-db.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "node dist/prisma/seed.js",
    "db:deduplicate": "ts-node -r tsconfig-paths/register scripts/deduplicate-contacts.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write .",
    "docker:build": "docker build -t flame-ai-backend .",
    "docker:run": "docker run -p 8080:8080 flame-ai-backend"
  },
  "keywords": [
    "whatsapp",
    "crm",
    "api",
    "typescript",
    "prisma",
    "express"
  ],
  "author": "Flame AI CRM Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "latest",
    "@whiskeysockets/baileys": "^6.6.0",
    "axios": "^1.7.2",
